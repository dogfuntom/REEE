{
	"manifest_version": 2,

	"name": "REEE",
	"description": "REcommendation Engine Extension",
	"homepage_url": "https://example.com/",

	"version": "0.1.0",

	"permissions": ["history", "storage"],

	"icons": {
		"72": "common/icon.png"
	},

	"browser_specific_settings": {
		"gecko": {
			"id": "reee@reee.com",
			"strict_min_version": "52.0"
		}
	},

	"browser_action": {
		"default_title": "REEE",
		"default_icon": {
			"72" : "common/icon.png"
		},
		"default_popup": "popup/index.html"
	}
}
